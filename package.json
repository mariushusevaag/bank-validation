{
  "name": "@husevaag.marius/bank-validation",
  "version": "1.1.6",
  "publishConfig": {
    "access": "public"
  },
  "description": "A package for validating banks, account numbers, OCR and more.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "types": "./dist/cjs/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mariushusevaag/bank-validation.git"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc -p tsconfig.esm.json",
    "changeset": "changeset",
    "prepublishOnly": "npm run build",
    "publish-package": "npm run build && changeset publish",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "validation",
    "bank",
    "account",
    "ocr"
  ],
  "author": "Marius Husev√•g",
  "license": "MIT",
  "type": "commonjs",
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@vitest/coverage-v8": "^3.0.5",
    "@changesets/cli": "^2.28.1",
    "typescript": "^4.7.0",
    "vitest": "^3.0.5"
  }
}